    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Odoo",
            "type": "debugpy",
            "request": "launch",
            "stopOnEntry": false,
            "python": "/opt/odoo17/odoo17-venv/bin/python3",
            "console": "integratedTerminal",
            // "program": "/opt/odoo17/odoo17/odoo-bin",
            "program": "/opt/odoo17/odoo17/odoo-bin",
            "args": [
                "-c /etc/odoo17.conf",
            ],
            "cwd": "${workspaceRoot}",
            "env": {},
            "envFile": "${workspaceRoot}/.env",
            "sudo": true
            // "debugOptions": [
